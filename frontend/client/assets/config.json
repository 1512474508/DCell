{
  "title": "DeepCell 1.0",
  "backendServices": {
    "db": "http://deep-cell-dev.ucsd.edu:9200",
    "simulator": "http://deep-cell-dev.ucsd.edu:8888"
  },

  "defaultNetwork": "go",


  "trees": {
    "go": {
      "name": "Gene Ontology",
      "rootNode": "GO:00SUPER",
      "searchOptions": {
        "index": "terms",
        "type": "go_term"
      },
      "properties": {
        "namespace": {
          "biological_process": {
            "tag": "BP",
            "color": "rgb(0,153,204)"
          },
          "cellular_component": {
            "tag": "CC",
            "color": "rgb(255,102,0)"
          },
          "molecular_function": {
            "tag": "MF",
            "color": "rgb(0,204,153)"
          }
        }
      },
      "url": "/data/tree-go-genes.json"
    },
    "clixo": {
      "name": "CLIXO",
      "rootNode": "CLIXO:10706",
      "searchOptions": {
        "index": "terms",
        "type": "clixo_term"
      },
      "properties": {
        "namespace": {}
      },
      "url": "/data/tree-clixo.json"
    }
  }
}
